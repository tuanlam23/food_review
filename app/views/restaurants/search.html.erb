<section class="reserve-block">
  <div class="container">
    <div class="row">
      <div class="col-12" style="text-align: center">
        <h2 class="yummy-logo">Yummy</h2>
      </div>
    </div>
  </div>
</section>
<section class="light-bg booking-details_wrap">
  <div class="container">
    <div class="row list mt-4">
      <div class="col-sm-12 yummy-toolbar">
        <div class="ym-clearbox" style="top: 0">
          <ul>
            <li><a href="#" class="current" data-url="<%= load_new_restaurants_path%>">Kết quả tìm kiếm</a></li>
          </ul>
        </div>
      </div>
      <div class="col-sm-12 pt-2">
        <div class="row" id="list_res">
          <% if @restaurants.blank? %>
            <h5>Không có dữ liệu phù hợp </h5>
          <% end %>

          <% @restaurants.each do |restaurant|%>
              <%= render partial: "add_restaurant", locals: {restaurant: restaurant} %>
          <% end %>
        </div>
        <!--<div class="row">-->
          <!--<div class="col-lg-12">-->
            <!--<a id="load-page" href="#" class="btn-more" data-page="2" >-->
              <!--Xem thêm...-->
            <!--</a>-->
          <!--</div>-->
        <!--</div>-->
      </div>
    </div>
  </div>
</section>