<div class="food-init">
  <div class="row">
    <div class="col-lg-12">
      <div class="form-group">
        <label>Tên đồ</label>
        <%= text_field_tag 'food[][name]', '', class: "form-control", :required => true%>
      </div>
    </div>
    <div class="col-lg-12">
      <div class="form-group">
        <label>Picture</label>
        <%= file_field_tag 'food[][picture]', :required => true%>
      </div>
    </div>
    <div class="col-lg-12">
      <div class="form-group">
        <label>Giá</label>
        <%= number_field_tag 'food[][price]', '', class: "form-control", :required => true%>
      </div>
    </div>
    <div class="col-lg-12">
      <a href="#" class="del-food" style="float: right">Xóa</a>
    </div>
  </div>
</div>
