<div class="food-init">
  <div class="row">
    <div class="col-lg-12">
      <div class="form-group">
        <label>Tên đồ</label>
        <%= text_field_tag 'food[][name]', '', class: "form-control"%>
      </div>
    </div>
    <div class="col-lg-12">
      <div class="form-group">
        <label>Picture</label>
        <%= file_field_tag 'food[][picture]', class: "form-control"%>
      </div>
    </div>
  </div>
</div>

